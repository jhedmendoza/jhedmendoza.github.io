        url('fonts/GameloftIcons.ttf?rhapg8') format('truetype'),
        url('fonts/GameloftIcons.woff?rhapg8') format('woff'),
        url('fonts/GameloftIcons.svg?rhapg8#icomoon') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'Gameloft Icons' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

