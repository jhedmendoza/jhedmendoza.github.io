// include all imported fonts here even google fonts
@charset 'utf-8';
/** font face **/
.font-a { font-family: 'Lunchtype'; }
@font-face {
  font-family: 'Lunchtype';
  src: url('fonts/Lunchtype.eot?#iefix') format('embedded-opentype'),  url('fonts/Lunchtype.woff') format('woff'), url('fonts/Lunchtype.ttf')  format('truetype'), url('fonts/Lunchtype.svg#Lunchtype') format('svg');
  font-weight: normal;
  font-style: normal;
}
@font-face {
  font-family: 'Lunchtype';
  src: url('fonts/Lunchtype-Bold.eot?#iefix') format('embedded-opentype'),  url('fonts/Lunchtype-Bold.woff') format('woff'), url('fonts/Lunchtype-Bold.ttf')  format('truetype'), url('fonts/Lunchtype-Bold.svg#Lunchtype-Bold') format('svg');
  font-weight: 700;
  font-style: normal;
}
/** font face **/
